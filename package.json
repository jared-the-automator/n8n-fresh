{
  "name": "n8n",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://n8n.io",
  "scripts": {
    "build": "pnpm run build:backend",
    "build:backend": "turbo run build --filter=\"@n8n/nodes-base\"",
    "dev": "pnpm run dev:backend",
    "dev:backend": "turbo run dev --parallel --filter=\"@n8n/nodes-base\"",
    "clean": "pnpm run clean:dist && pnpm run clean:coverage",
    "clean:dist": "rimraf packages/*/dist",
    "clean:coverage": "rimraf packages/*/coverage",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "prettier": "^2.8.3",
    "rimraf": "^3.0.2",
    "turbo": "^1.10.16",
    "typescript": "~5.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=16.9.0",
    "pnpm": ">=8.9.0"
  },
  "packageManager": "pnpm@8.9.0"
}
